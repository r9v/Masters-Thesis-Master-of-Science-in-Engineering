figure;
i=0;
mytime=0;
j=1;
while mytime<=10
    i=i+1;
    if(q.Time(i)>=mytime) 
        mytime=mytime+0.5;
        timeIndexVector(i)=i;
    end
end
while mytime<=10
    i=i+2;
    if(q.Time(i)>=mytime) 
        mytime=mytime+2;
        [Base,K1,K2,C1,C2,C3,C4]=coords(q.Data(i,:));
        drawRobot(Base,K1,K2,C1,C2,C3,C4)
        scatter(K2(1),K2(2),'b','filled')
        %pause(0.5)
    end
end
ylabel('y[m]')
